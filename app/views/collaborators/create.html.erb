<h1> Add Collaborator </h1>
   <%= form_for [ @wiki, @collaborator] do |form| %>
     <%= form.label :name %>
     <%= form.label :email %>
     <p>
     <%= @user.email%> will be  added as a collaborator to wiki <%= @wiki.title %> ?
     <%#= form.wiki_id hidden:true %>
     <%#= form.user_id hidden:true %>
   </p>
     <div>
      <%= form.submit %>
     </div>
 <% end %>