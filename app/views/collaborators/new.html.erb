<h1> Add Collaborator </h1>

   <%= form_for [@wiki, @collaborator] do |form| %>
     <%#= form.label :name %>
     <%#= form.label :email %>
     <p>
     Add collaborator <%= @user.email %> to wiki <%= @wiki.title %> ?
     <%#=form.wiki_id hidden:true %>
     <%= form.wiki.user_id hidden:true %>
    </p>    
     <div>
      <%= form.submit %>
     </div>
 <% end %>
     
